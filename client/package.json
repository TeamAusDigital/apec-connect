{
  "private": true,
  "name": "apec-connect-client",
  "version": "0.0.1",
  "description": "APEC Connect - Client",
  "homepage": "http://apec-connect.com",
  "watch": {
    "dev:server": "tmp/bundle.js"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:js": "rimraf public/js/main*.js && rimraf public/js/app.js && rimraf public/js/plugins.js && rimraf public/js/*.map",
    "clean:css": "rimraf public/css/main*.css && rimraf public/css/app*.css && rimraf public/css/*.map",
    "clean:fonts": "rimraf public/fonts/*",
    "clean:server": "rimraf server.js && rimraf server.js.map",
    "clean:tmp": "rimraf tmp",
    "clean": "run-s clean:dist clean:tmp clean:server clean:css clean:js clean:fonts",
    "update:schema": "babel-node tools/update-schema.js",
    "hot:client": "cross-env NODE_ENV=development EXTRACT_TEXT_PLUGIN=false webpack-dev-server --verbose --config tools/webpack.client.js",
    "hot:server": "cross-env NODE_ENV=development webpack --colors --display-error-details --watch --verbose --config tools/webpack.server.js",
    "dev:server": "cross-env NODE_ENV=development node tmp/bundle.js",
    "dev:server:wait": "just-wait -p \"tmp/bundle.js\" --timeout 120 && npm run start-watcher",
    "start-watcher": "npm-watch",
    "watch-build-server": "run-s clean:tmp hot:server",
    "dev": "rimraf tmp && concurrently \"npm run check-type\" \"npm run lint\" \"npm run watch-build-server\" \"npm run hot:client\" \"npm run dev:server:wait\"",
    "dev:dist": "run-p dev dist:watch",
    "build:server": "cross-env NO_OUTPUT_PATH=true NODE_ENV=production webpack --progress --colors --display-error-details --config tools/webpack.server.js",
    "build:client": "cross-env EXTRACT_TEXT_PLUGIN=true webpack --progress --colors --display-error-details --config tools/webpack.client.js",
    "bundle:server": "run-s clean:tmp clean:server build:server",
    "bundle:client": "run-s clean:css clean:js clean:fonts build:client",
    "prod:server": "cross-env NODE_ENV=production node server.js",
    "build:prod": "cross-env NODE_ENV=production run-s check-type lint test bundle:client bundle:server",
    "prod": "run-s build:prod prod:server",
    "dist": "cross-env NODE_ENV=production run-s bundle:client && node tools/create-dist.js",
    "dist:watch": "run-s bundle:client && node tools/watch-dist.js",
    "test": "jest --collectCoverageFrom='[\"src/**/*\"]' --coverage",
    "start": "node server.js",
    "check-type": "flow",
    "lint": "eslint ./src/**"
  },
  "author": "APEC Connect",
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-eslint": "6.1.2",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.16.0",
    "bless-webpack-plugin": "1.0.0",
    "chokidar": "1.6.1",
    "concurrently": "2.2.0",
    "cross-env": "1.0.8",
    "css-loader": "0.23.1",
    "deepmerge": "0.2.10",
    "enzyme": "2.5.1",
    "eslint": "3.19.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.11.1",
    "flow-bin": "0.34.0",
    "imports-loader": "0.7.0",
    "isomorphic-style-loader": "1.0.0",
    "istanbul-reports": "^1.1.0",
    "jasmine": "2.5.2",
    "jasmine-reporters": "2.2.0",
    "jest": "16.0.2",
    "jsdom": "9.8.3",
    "json-loader": "0.5.4",
    "loader-utils": "0.2.16",
    "ncp": "2.0.0",
    "node-sass": "3.10.1",
    "npm-run-all": "2.3.0",
    "npm-watch": "0.1.6",
    "null-loader": "0.1.1",
    "postcss-loader": "0.9.1",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.3.2",
    "redux-saga-test-plan": "^2.4.4",
    "rimraf": "2.5.4",
    "rtlcss": "2.0.6",
    "sass-loader": "3.2.3",
    "script-loader": "0.7.0",
    "style-loader": "0.13.1",
    "uglify-loader": "1.3.0",
    "url-loader": "0.5.7",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "axios": "0.13.1",
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-relay-plugin-loader": "0.9.0",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "es6-promise": "3.3.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "4.14.0",
    "fetch-it": "0.3.0",
    "font-awesome": "^4.7.0",
    "immutable": "3.8.1",
    "jquery": "^3.2.1",
    "json2csv": "3.7.3",
    "just-wait": "^1.0.11",
    "material-ui": "^0.18.0",
    "modernizr": "^3.5.0",
    "moment": "2.15.2",
    "prop-types": "^15.5.8",
    "pug": "2.0.0-beta6",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "3.0.0-beta.6",
    "react-js-pagination": "2.0.1",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router": "3.0.4",
    "react-router-dom": "^4.1.1",
    "react-scroll": "1.5.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-actions": "0.12.0",
    "redux-persist": "^4.8.0",
    "redux-router": "^2.1.2",
    "redux-saga": "^0.15.1",
    "source-map-support": "0.4.5",
    "susy": "^2.2.12",
    "valuelink": "^1.4.3",
    "webpack": "1.13.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "tests/setup-jasmine-env.js"
  }
}
